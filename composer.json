{
  "name": "epalshin/enum",
  "description": "Powerful enum library for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Palshin",
      "email": "eo.palshin@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Palshin\\Enum\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Palshin\\Enum\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": "^9.5",
    "spatie/ray": "^1.10",
    "vimeo/psalm": "^4.3"
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "psalm": "vendor/bin/psalm",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
